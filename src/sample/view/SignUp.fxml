<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DatePicker?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="-Infinity" maxWidth="-Infinity"
            minHeight="-Infinity" minWidth="-Infinity"
            prefHeight="400.0" prefWidth="700.0"
            style="-fx-background-color: #3CB371;"
            xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="sample.controller.SignUpController">
    <children>
        <AnchorPane layoutY="78.0" prefHeight="322.0"
                    prefWidth="700.0" style="-fx-background-color: E0FFFF;">
            <children>
                <TextField fx:id="SurName" layoutX="66.0"
                           layoutY="64.0" prefHeight="38.0"
                           prefWidth="150.0" promptText="Фамилия">
                    <font>
                        <Font name="Constantia" size="20.0" />
                    </font>
                </TextField>
                <Label layoutX="115.0" layoutY="14.0" text="Регистрация">
                    <font>
                        <Font name="Corbel Light" size="32.0" />
                    </font>
                </Label>
                <Button fx:id="AuthSignInButton" layoutX="452.0"
                        layoutY="226.0" mnemonicParsing="false"
                        prefHeight="25.0" prefWidth="216.0"
                        style="-fx-background-color: #FA8072; -fx-text-fill: E0FFFF;"
                        text="Зарегистрироваться">
                    <font>
                        <Font size="17.0" />
                    </font>
                </Button>
            <TextField fx:id="Name" layoutX="223.0"
                       layoutY="64.0" prefHeight="38.0"
                       prefWidth="150.0" promptText="Имя">
               <font>
                  <Font name="Constantia" size="20.0" />
               </font>
            </TextField>
            <PasswordField fx:id="Password" layoutX="66.0"
                           layoutY="226.0" prefHeight="38.0"
                           prefWidth="308.0" promptText="Пароль">
               <font>
                  <Font name="Constantia" size="20.0" />
               </font>
            </PasswordField>
            <TextField fx:id="MiddleName" layoutX="66.0"
                       layoutY="126.0" prefHeight="38.0"
                       prefWidth="150.0" promptText="Очество">
               <font>
                  <Font name="Constantia" size="20.0" />
               </font>
            </TextField>
            <DatePicker fx:id="Birthday" layoutX="66.0"
                        layoutY="179.0" prefHeight="38.0"
                        prefWidth="308.0" promptText="Дата рождения" />
            <ImageView fx:id="AddPhotoButton" fitHeight="150.0"
                       fitWidth="150.0" layoutX="485.0"
                       layoutY="51.0" pickOnBounds="true"
                       preserveRatio="true">
               <image>
                  <Image url="@../assets/PhotoAccount.png" />
               </image>
            </ImageView>
            <TextField fx:id="Position" layoutX="223.0"
                       layoutY="126.0" prefHeight="38.0"
                       prefWidth="150.0" promptText="Должость">
               <font>
                  <Font name="Constantia" size="20.0" />
               </font>
            </TextField>
            </children>
        </AnchorPane>
        <Label layoutX="242.0" layoutY="14.0" text="Test JavaFX">
            <font>
                <Font name="Corbel Light" size="46.0" />
            </font>
        </Label>
        <Button fx:id="LognSignUpButton" layoutX="552.0"
                layoutY="20.0" mnemonicParsing="false"
                prefHeight="40.0" prefWidth="133.0"
                style="-fx-background-color: #AFEEEE; -fx-text-fill: #3CB371;"
                text="Войти" textFill="#030303">
            <font>
                <Font size="17.0" />
            </font>
        </Button>
    </children>
</AnchorPane>
